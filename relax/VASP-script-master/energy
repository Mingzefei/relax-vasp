#!/bin/bash
echo ""
if [ -e OUTCAR ]
then
    tac OUTCAR |grep -m 1 "energy  with"
elif [ "$1" != "" ]
then
    for i in $@
    do
        for j in `find $i -name OUTCAR |sort`
        do
            echo "$j"
            tac $j |grep -m 1 "energy  with"
        done
    done
else
    for i in `find ./ -name OUTCAR |sort`
    do
        echo "$i"
        tac $i |grep -m 1 "energy  with"
    done
fi
echo ""
